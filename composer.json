{
  "name": "ibrand/discount",
  "type": "library",
  "description": "ibrand discount component",
  "keywords": [
    "ibrand",
    "discount",
    "component"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shjchen",
      "email": "ibrand.shjchen@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ibrand/laravel-cache": "~1.0.0",
    "prettus/l5-repository": "^2.1"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "iBrand\\Component\\Discount\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iBrand\\Component\\Discount\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "iBrand\\Component\\Discount\\Providers\\DiscountServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}